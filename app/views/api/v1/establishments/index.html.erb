<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Establishments</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_establishments.each do |api_v1_establishment| %>
      <tr>
        <td><%= link_to 'Show', api_v1_establishment %></td>
        <td><%= link_to 'Edit', edit_api_v1_establishment_path(api_v1_establishment) %></td>
        <td><%= link_to 'Destroy', api_v1_establishment, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Establishment', new_api_v1_establishment_path %>
