<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Menus</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_menus.each do |api_v1_menu| %>
      <tr>
        <td><%= link_to 'Show', api_v1_menu %></td>
        <td><%= link_to 'Edit', edit_api_v1_menu_path(api_v1_menu) %></td>
        <td><%= link_to 'Destroy', api_v1_menu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Menu', new_api_v1_menu_path %>
